{
  "name": "@synerise/ds-column-manager",
  "version": "0.2.6",
  "description": "ColumnManager UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.12.0",
    "@synerise/ds-drawer": "^0.4.13",
    "@synerise/ds-dropdown": "^0.10.10",
    "@synerise/ds-icon": "^0.11.1",
    "@synerise/ds-item-filter": "^0.2.45",
    "@synerise/ds-result": "^0.4.33",
    "@synerise/ds-scrollbar": "^0.0.11",
    "@synerise/ds-search-bar": "^0.2.32",
    "@synerise/ds-typography": "*",
    "react-final-form": "^6.3.5",
    "react-sortablejs": "^2.0.11",
    "react-sortablejs-typescript": "^2.0.15"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  },
  "devDependencies": {
    "moment": "^2.24.0"
  }
}
