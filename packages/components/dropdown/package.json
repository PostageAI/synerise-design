{
  "name": "@synerise/ds-dropdown",
  "version": "0.15.9",
  "description": "Dropdown UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-divider": "^0.4.54",
    "@synerise/ds-icon": "^0.43.0",
    "@synerise/ds-input": "^0.17.9",
    "@synerise/ds-search-bar": "^0.3.13"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "@synerise/ds-typography": "^0.10.0",
    "antd": "^4",
    "react": ">=16.9.0 < 17.0.0"
  },
  "devDependencies": {
    "@synerise/ds-utils": "^0.14.0"
  }
}
