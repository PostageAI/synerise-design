{
  "name": "@synerise/ds-condition",
  "version": "0.6.20",
  "description": "Condition UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.13.44",
    "@synerise/ds-context-selector": "^0.4.15",
    "@synerise/ds-core": "^0.30.1",
    "@synerise/ds-cruds": "^0.2.23",
    "@synerise/ds-factors": "^0.6.15",
    "@synerise/ds-icon": "^0.36.0",
    "@synerise/ds-inline-edit": "^0.4.2",
    "@synerise/ds-operators": "^0.3.17",
    "@synerise/ds-subject": "^0.5.17",
    "@synerise/ds-tooltip": "^0.10.29",
    "@synerise/ds-utils": "^0.14.2",
    "react-sortablejs": "^6.0.0"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  }
}
