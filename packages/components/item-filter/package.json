{
  "name": "@synerise/ds-item-filter",
  "version": "0.4.22",
  "description": "ItemFilter UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.12.24",
    "@synerise/ds-drawer": "^0.4.27",
    "@synerise/ds-dropdown": "^0.11.62",
    "@synerise/ds-icon": "^0.11.1",
    "@synerise/ds-manageable-list": "^0.13.8",
    "@synerise/ds-result": "^0.4.58",
    "@synerise/ds-scrollbar": "^0.2.10",
    "@synerise/ds-search-bar": "^0.2.68",
    "@synerise/ds-tabs": "^0.10.22",
    "@synerise/ds-typography": "^0.8.3",
    "react-window": "^1.8.5"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  }
}
