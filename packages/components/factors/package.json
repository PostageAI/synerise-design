{
  "name": "@synerise/ds-factors",
  "version": "0.8.0",
  "description": "Factors UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-autocomplete": "^0.3.79",
    "@synerise/ds-badge": "^0.4.81",
    "@synerise/ds-button": "^0.13.67",
    "@synerise/ds-date-picker": "^0.5.66",
    "@synerise/ds-dropdown": "^0.16.28",
    "@synerise/ds-icon": "^0.45.0",
    "@synerise/ds-inline-edit": "^0.4.36",
    "@synerise/ds-input": "^0.17.43",
    "@synerise/ds-input-number": "^0.5.23",
    "@synerise/ds-menu": "^0.10.42",
    "@synerise/ds-modal": "^0.14.12",
    "@synerise/ds-result": "^0.5.75",
    "@synerise/ds-tabs": "^0.11.71",
    "@synerise/ds-typography": "^0.11.9",
    "@synerise/ds-utils": "^0.19.0",
    "classnames": "^2.2.6",
    "react-window": "1.8.5"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  }
}
