{
  "name": "@synerise/ds-factors",
  "version": "0.2.17",
  "description": "Factors UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-autocomplete": "^0.3.48",
    "@synerise/ds-badge": "^0.4.50",
    "@synerise/ds-button": "^0.13.24",
    "@synerise/ds-date-picker": "^0.5.11",
    "@synerise/ds-dropdown": "^0.13.14",
    "@synerise/ds-icon": "^0.37.0",
    "@synerise/ds-inline-edit": "^0.2.16",
    "@synerise/ds-input": "^0.16.17",
    "@synerise/ds-input-number": "^0.4.4",
    "@synerise/ds-menu": "^0.8.0",
    "@synerise/ds-modal": "^0.13.8",
    "@synerise/ds-result": "^0.5.31",
    "@synerise/ds-tabs": "^0.11.16",
    "@synerise/ds-typography": "^0.10.0",
    "@synerise/ds-utils": "^0.11.12"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  }
}
