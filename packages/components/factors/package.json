{
  "name": "@synerise/ds-factors",
  "version": "0.1.30",
  "description": "Factors UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-autocomplete": "^0.3.43",
    "@synerise/ds-badge": "^0.4.45",
    "@synerise/ds-button": "^0.13.12",
    "@synerise/ds-date-picker": "^0.2.20",
    "@synerise/ds-dropdown": "^0.11.97",
    "@synerise/ds-icon": "^0.34.0",
    "@synerise/ds-inline-edit": "^0.1.91",
    "@synerise/ds-input": "^0.16.3",
    "@synerise/ds-input-number": "^0.3.43",
    "@synerise/ds-menu": "^0.7.6",
    "@synerise/ds-modal": "^0.12.17",
    "@synerise/ds-result": "^0.5.19",
    "@synerise/ds-tabs": "^0.10.67",
    "@synerise/ds-typography": "^0.9.5",
    "@synerise/ds-utils": "^0.11.7"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  }
}
