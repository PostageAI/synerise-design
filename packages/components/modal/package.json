{
  "name": "@synerise/ds-modal",
  "version": "0.2.3",
  "description": "Modal UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "synerise/synerise-design",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll"
  },
  "sideEffects": false,
  "dependencies": {
    "@synerise/ds-button": "^0.2.3",
    "antd": "*"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  }
}
