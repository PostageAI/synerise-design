{
  "name": "@synerise/ds-sidebar",
  "version": "0.1.0",
  "description": "Sidebar UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-block": "^0.1.0",
    "@synerise/ds-button": "^0.5.0",
    "@synerise/ds-card": "^0.2.17",
    "@synerise/ds-icon": "^0.5.0",
    "@synerise/ds-typography": "^0.6.0",
    "antd": "3.26.0",
    "immutability-helper": "^3.0.1",
    "react-dnd": "^9.5.1",
    "react-dnd-html5-backend": "^9.5.1"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  }
}
