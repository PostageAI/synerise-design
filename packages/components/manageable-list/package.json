{
  "name": "@synerise/ds-manageable-list",
  "version": "0.10.15",
  "description": "ManageableList UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.9.8",
    "@synerise/ds-icon": "^0.14.2",
    "@synerise/ds-list": "^0.4.7",
    "@synerise/ds-popconfirm": "^0.3.8",
    "@synerise/ds-tags": "^0.2.48",
    "@synerise/ds-tooltip": "^0.7.0",
    "@synerise/ds-typography": "^0.8.2"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "@synerise/ds-inline-edit": "^0.0.15",
    "antd": "^4",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "^4.4.0"
  }
}
