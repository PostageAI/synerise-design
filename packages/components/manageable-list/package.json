{
  "name": "@synerise/ds-manageable-list",
  "version": "0.17.7",
  "description": "ManageableList UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-avatar": "^0.10.12",
    "@synerise/ds-button": "^0.13.34",
    "@synerise/ds-dropdown": "^0.15.1",
    "@synerise/ds-icon": "^0.39.0",
    "@synerise/ds-inline-edit": "^0.3.4",
    "@synerise/ds-input": "^0.17.1",
    "@synerise/ds-list": "^0.5.33",
    "@synerise/ds-menu": "^0.10.4",
    "@synerise/ds-modal": "^0.13.18",
    "@synerise/ds-popconfirm": "^0.6.5",
    "@synerise/ds-result": "^0.5.41",
    "@synerise/ds-tags": "^0.3.24",
    "@synerise/ds-tooltip": "^0.10.18",
    "@synerise/ds-typography": "^0.10.2",
    "@synerise/ds-utils": "^0.13.0",
    "lodash": "^4.17.19",
    "react-animate-height": "^2.0.23",
    "react-sortablejs": "^6.0.0"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "@synerise/ds-inline-edit": "^0.0.15",
    "antd": "^4",
    "react": ">=16.9.0 < 17.0.0",
    "styled-components": "^4.4.0"
  }
}
