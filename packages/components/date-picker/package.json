{
  "name": "@synerise/ds-date-picker",
  "version": "0.1.0",
  "description": "DatePicker UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "types": "tsc --noEmit"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@synerise/ds-button": "^0.12.18",
    "@synerise/ds-core": "^0.15.1",
    "@synerise/ds-icon": "^0.19.0",
    "@synerise/ds-time-picker": "^0.2.43",
    "@synerise/ds-tooltip": "^0.7.6",
    "classnames": "^2.2.6",
    "date-fns": "1.30.1",
    "moment": "^2.26.0",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-day-picker": "^7.4.8"
  },
  "peerDependencies": {
    "@synerise/ds-core": "*",
    "react": ">=16.9.0 < 17.0.0"
  }
}
