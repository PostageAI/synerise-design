{
  "name": "@synerise/ds-charts",
  "version": "0.0.1",
  "description": "Charts UI Component for the Synerise Design System",
  "license": "ISC",
  "repository": "Synerise/synerise-design",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run defs",
    "build:css": "node ../../../scripts/style/less.js",
    "build:js": "babel --root-mode upward src --out-dir dist --extensions '.js,.ts,.tsx'",
    "build:watch": "npm run build:js -- --watch",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "prepublish": "npm run build",
    "types": "tsc --noEmit",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll"
  },
  "sideEffects": [
    "dist/style/*",
    "*.less"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@rebass/grid": "^6.1.0",
    "@synerise/ds-core": "*",
    "@synerise/ds-loader": "^0.1.30",
    "@synerise/ds-modal": "^0.14.10",
    "@synerise/ds-menu": "^0.10.41",
    "@synerise/ds-tooltip": "^0.10.51",
    "highcharts": "^9.2.2",
    "highcharts-custom-events": "^3.0.10",
    "lodash": "^4.17.21",
    "react-highcharts": "^16.1.0",
    "react-jsx-highstock": "^4.3.1"
  },
  "peerDependencies": {
    "react": ">=16.9.0 < 17.0.0"
  },
  "devDependencies": {
    "@types/react-highcharts": "^16.0.3",
    "@types/rebass__grid": "^6.0.7"
  }
}
